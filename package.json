{
  "name": "ordermind",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:migrate": "pnpm --filter @ordermind/api db:migrate",
    "db:generate": "pnpm --filter @ordermind/api db:generate",
    "db:seed": "pnpm --filter @ordermind/api db:seed",
    "db:studio": "pnpm --filter @ordermind/api db:studio",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
